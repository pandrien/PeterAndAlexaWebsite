<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<rewrite>
		    <rules>
			<rule name="SpecificRewrite" stopProcessing="true">
			    <match url="^yourpage$" />
			    <action type="Rewrite" url="/yourpage.html" />
			</rule>
		    </rules>
		</rewrite>
	</system.webServer>
</configuration>
