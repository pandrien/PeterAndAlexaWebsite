<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<rewrite>
			<rules>
				<rule name="rule 1E" stopProcessing="true">
					<match url="^([^\.]+)$"  ignoreCase="true" />
					<action type="Rewrite" url="/{R:1}.php"  />
				</rule> 
			</rules>
		</rewrite>
	</system.webServer>
</configuration>
